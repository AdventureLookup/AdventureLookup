{
  "private": true,
  "scripts": {
    "dev": "encore dev -w",
    "dev-server-guest": "encore dev-server --hot --host 0.0.0.0 --port 8001",
    "dev-server-host": "encore dev-server --hot --port 8002",
    "dev-server-gitpod": "encore dev-server --hot --port 8002 --public $(gp url 8002) --disable-host-check",
    "format": "prettier --write app/Resources/**/*.{js,jsx,scss}",
    "lint": "prettier --check app/Resources/**/*.{js,jsx,scss}",
    "build": "encore production"
  },
  "dependencies": {
    "autosize": "^5.0.1",
    "bootstrap": "^4.6.1",
    "cookieconsent": "^3.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "match-sorter": "^6.3.1",
    "popper.js": "^1.16.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "selectize": "^0.12.6",
    "toastr": "^2.1.4",
    "vanilla-lazyload": "^8.17.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.0",
    "@symfony/webpack-encore": "^1.7.0",
    "autoprefixer": "^10.4.0",
    "husky": "^4.3.8",
    "less": "^4.1.2",
    "lint-staged": "^10.5.4",
    "node-sass": "^7.0.0",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "sass-loader": "^12.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "scripts/lint-staged.sh"
    }
  },
  "lint-staged": {
    "app/Resources/**/*.{js,jsx,scss}": "prettier --write",
    "{src,tests}/**/*.php": "composer run format --"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "engines": {
    "npm": "6.x",
    "node": "12.x"
  }
}
